//Form  1 .cs 

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace Zadatak2
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void Form1_Load(object sender, EventArgs e)
        {
            dataGridView1.Top = 20;
            dataGridView1.Left = 20;
            dataGridView1.ScrollBars = ScrollBars.None;
            dataGridView1.ColumnHeadersVisible = false;
            dataGridView1.RowHeadersVisible = false;
        }

        private void button1_Click(object sender, EventArgs e)
        {
            int i, j;
            Random r = new Random();
            int n = (int)numericUpDown1.Value;
            dataGridView1.RowCount = n;
            dataGridView1.ColumnCount = n;
            dataGridView1.Height = n * 20 + 3;
            dataGridView1.Width = n * 20 + 3;
            for (i = 0; i < n; i++)
                dataGridView1.Rows[i].Height = 20;
            for (j = 0; j < n; j++)
                dataGridView1.Columns[j].Width = 20;
            for (i = 0; i < n; i++)
                for (j = 0; j < n; j++)
                    dataGridView1[j, i].Value = r.Next(1, 100);
        }

        private void button2_Click(object sender, EventArgs e)
        {
            int n = (int)numericUpDown1.Value;
            dataGridView1.RowCount = n;
            dataGridView1.ColumnCount = n;
            dataGridView1.Height = n * 20 + 3;
            dataGridView1.Width = n * 20 + 3;
            int i, j, s = 0;
            for (i = 0; i < n; i++)
                for (j = 0; j < n; j++)
                    s += (int)dataGridView1[j, i].Value;
            double prosek = s / (n * n);
            listBox1.Items.Add(prosek.ToString());
        }

        private void button3_Click(object sender, EventArgs e)
        {
            this.Close();
        }

        private void button4_Click(object sender, EventArgs e)
        {
            int n = (int)numericUpDown1.Value;
            dataGridView1.RowCount = n;
            dataGridView1.ColumnCount = n;
            dataGridView1.Height = n * 20 + 3;
            dataGridView1.Width = n * 20 + 3;
            int i;
            int j;
            int min = (int)dataGridView1[0, 0].Value;
            for (i = 0; i < n; i++)
                for (j = 0; j < n; j++)
                    if (min > (int)dataGridView1[i, j].Value)
                    {
                        min = (int)dataGridView1[i, j].Value;

                    }
            listBox1.Items.Add("Najmanji broj je" + " " + min);


        }

        private void button5_Click(object sender, EventArgs e)
        {
            int n = (int)numericUpDown1.Value;
            dataGridView1.RowCount = n;
            dataGridView1.ColumnCount = n;
            dataGridView1.Height = n * 20 + 3;
            dataGridView1.Width = n * 20 + 3;
            int i;
            int j;
            int zbir = 0;

            for (i = 0; i < n; i++)
            {
                for (j = 0; j < n; j++)
                {
                    if (10 > ((int)dataGridView1[i, j].Value))
                    {
                        zbir += ((int)dataGridView1[i, j].Value);
                    }
                }
            }

            listBox1.Items.Add("Zbir brojeva manjih od 10 je" + " " + zbir);
        }

        private void button6_Click(object sender, EventArgs e)
        {
            int n = (int)numericUpDown1.Value;
            dataGridView1.RowCount = n;
            dataGridView1.ColumnCount = n;
            dataGridView1.Height = n * 20 + 3;
            dataGridView1.Width = n * 20 + 3;
            int i;
            int j;
            for (j = 0; j < n; j++)
                for (i = 0; i < n; i++)
                {

                    dataGridView1[i, j].Value = ((int)dataGridView1[j, i].Value);

                }



        }

        private void button7_Click(object sender, EventArgs e)
        {



        
    }

    }
    }




